{
	"easycom": {
	      "autoscan": true,
	      "custom": {
	          // uni-ui 规则如下配置
	          "^vue-file-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
	      
	  }
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/home/home",
			"style": {
				"navigationStyle": "custom",
				"navigationBarTitleText": "首页"
			},
			"h5":{
				"titleNView": false
			}
		},
		{
			"path": "pages/classify/classify",
			"style": {
				"navigationBarTitleText": "分类"
			}
		},
		{
			"path": "pages/message/message",
			"style": {
				"navigationBarTitleText": "消息"
			}
		},
		{
			"path": "pages/cart/cart",
			"style": {
				"navigationBarTitleText": "购物车"
			}
		},
		{
			"path": "pages/my/my",
			"style": {
				"navigationStyle": "custom"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
			"color":"#8a8a8a",
			"selectedColor":"#288851",
			"borderStyle":"black",
			"backgroundColor":"#ffffff",
			"list": [
				{
					"pagePath":"pages/home/home",
					"text":"首页",
					"iconPath":"static/tabBar/home.png",
					"selectedIconPath":"static/tabBar/homefill.png"
				},
				{
					"pagePath":"pages/classify/classify",
					"text":"分类",
					"iconPath":"static/tabBar/circle.png",
					"selectedIconPath":"static/tabBar/circle_fill.png"
				},
				{
					"pagePath":"pages/message/message",
					"text":"消息",
					"iconPath":"static/tabBar/message.png",
					"selectedIconPath":"static/tabBar/message_fill_light.png"
				},
				{
					"pagePath":"pages/cart/cart",
					"text":"购物车",
					"iconPath":"static/tabBar/cart.png",
					"selectedIconPath":"static/tabBar/cartfill.png"
				},
				{
					"pagePath":"pages/my/my",
					"text":"我的",
					"iconPath":"static/tabBar/people.png",
					"selectedIconPath":"static/tabBar/peoplefill.png"
				}
				]
				}
				
				    
}
